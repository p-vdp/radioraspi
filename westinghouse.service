[Unit]
Description=westinghouse
Wants=dynamicswap.service

StartLimitBurst=1
StartLimitIntervalSec=10

[Service]
ExecStart=/usr/bin/python /home/rpi/westinghouse/westinghouse.py
Restart=always

StandardOutput=syslog
StandardError=syslog
SyslogIdentifier=westinghouse
User=root
Group=root

[Install]
WantedBy=multi-user.target
